{
  "name": "titan-technology-corporation-website",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.3",
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=9.15",
    "node": ">=v20.18"
  },
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "dev": "npx -y only-allow pnpm && next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write ./**/*.{ts,tsx,css,scss}",
    "check-format": "prettier --check ./**/*.{ts,tsx,css,scss}",
    "eslint": "eslint ./**/*.ts",
    "eslint:fix": "eslint ./**/*.ts --fix",
    "check-all": "pnpm check-format && pnpm eslint && pnpm lint && tsc -p tsconfig.json --noEmit && echo Code check: OK!",
    "fix-all": "pnpm run format && pnpm run eslint:fix"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not dead",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.13.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@marsidev/react-turnstile": "^1.1.0",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@mui/system": "^6.2.1",
    "@mui/x-date-pickers": "^7.27.0",
    "@next/eslint-plugin-next": "^15.1.1",
    "@next/third-parties": "^15.1.4",
    "aos": "^2.3.4",
    "card-validator": "^10.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.5",
    "html-react-parser": "^5.2.1",
    "jarallax": "^2.1.4",
    "leaflet": "^1.9.4",
    "next": "14.2.18",
    "next-i18n-router": "^5.5.1",
    "node-mailjet": "^6.0.6",
    "nodemailer": "^6.9.16",
    "react": "18.3.1",
    "react-countup": "^6.5.0",
    "react-datepicker": "^8.1.0",
    "react-dom": "18.3.1",
    "react-leaflet": "4.2.1",
    "react-slick": "^0.30.2",
    "react-syntax-highlighter": "^15.5.0",
    "react-visibility-sensor": "^5.1.1",
    "sharp": "^0.33.5",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.13",
    "tw-colors": "^3.3.2",
    "yup": "^1.3.2",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-syntax-flow": "^7.23.3",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@types/aos": "^3.0.7",
    "@types/leaflet": "^1.9.16",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "postcss": "^8.4.49",
    "prettier": "^3.1.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.16",
    "typescript": "^5",
    "yarn-upgrade-all": "^0.7.2"
  }
}